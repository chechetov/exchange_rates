<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
	<h2>Currencies operations</h2>
</div>

<div class="card text-center shadow p-3 mb-5 bg-white rounded">
  <div class="card-body ">
    <div class="d-flex justify-content-center">
      <div class="field form-group">
      	<%= link_to({:controller => 'currencies', :action => 'list'}) do %>
      	<button type="button" class="btn btn-success">List</button>
      	<% end %>
      	<%= link_to({:controller => 'currencies', :action => 'get'}) do %>
      	<button type="button" class="btn btn-success">Update</button>
      	<% end %>
      	<%= link_to({:controller => 'currencies', :action => 'delete'}) do %>
      	<button type="button" class="btn btn-danger">Clear</button>
      	<% end %>
    </div>
  </div>
  </div>
</div>





